<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.

    -->
   

    <!-- Application theme. -->


    <style name="Fade">
        <item name="android:windowEnterAnimation">@anim/abc_fade_in</item>
        <item name="android:windowExitAnimation">@anim/abc_fade_out</item>
    </style>


    <style name="SlideLeft">
        <item name="android:windowEnterAnimation">@anim/slide_in_left</item>
        <item name="android:windowExitAnimation">@anim/slide_out_left</item>
    </style>


    <style name="SlideRight">
        <item name="android:windowEnterAnimation">@anim/slide_in_right</item>
        <item name="android:windowExitAnimation">@anim/slide_out_right</item>
    </style>

    <style name="Leftinrightout">
        <item name="android:windowEnterAnimation">@anim/slide_in_left</item>
        <item name="android:windowExitAnimation">@anim/slide_out_right</item>
    </style>


    <style name="Rightinleftout">
        <item name="android:windowEnterAnimation">@anim/slide_in_right</item>
        <item name="android:windowExitAnimation">@anim/slide_out_left</item>
    </style>


    <style name="SlideTop">
        <item name="android:windowEnterAnimation">@anim/push_down_in</item>
        <item name="android:windowExitAnimation">@anim/push_up_out</item>
    </style>


    <style name="SlideBottom">
        <item name="android:windowEnterAnimation">@anim/push_up_in</item>
        <item name="android:windowExitAnimation">@anim/push_down_out</item>
    </style>

    <style name="BottominToppout">
        <item name="android:windowEnterAnimation">@anim/push_up_in</item>
        <item name="android:windowExitAnimation">@anim/push_up_out</item>
    </style>

    <style name="TopinBottomout">
        <item name="android:windowEnterAnimation">@anim/push_down_in</item>
        <item name="android:windowExitAnimation">@anim/push_down_out</item>
    </style>

</resources>